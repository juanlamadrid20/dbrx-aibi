{
  "datasets": [
    {
      "name": "9ceb519e",
      "displayName": "List of Tables -- Permissions",
      "queryLines": [
        "SELECT\n",
        "    grantor, grantee, concat(TABLE_CATALOG, '.', TABLE_SCHEMA, '.', TABLE_NAME) AS full_name,\n",
        "    table_catalog, table_schema, table_name, privilege_type\n",
        "FROM \n",
        "    system.information_schema.table_privileges\n",
        "WHERE \n",
        "    (grantee = :account_user or :account_user = \"All Grantees\") AND\n",
        "    ((:privilege_type = \"All Types\") \n",
        "      OR (:privilege_type = \"All Privileges\" AND privilege_type = \"ALL_PRIVILEGES\") \n",
        "      OR (:privilege_type = \"Write\" AND privilege_type = \"MODIFY\") \n",
        "      OR (:privilege_type = \"Read\" AND privilege_type = \"SELECT\"))\n",
        "    AND (grantor = :grantor OR :grantor = \"All Grantors\");"
      ],
      "parameters": [
        {
          "displayName": "account_user",
          "keyword": "account_user",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "account users"
                }
              ]
            }
          }
        },
        {
          "displayName": "privilege_type",
          "keyword": "privilege_type",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "All Types"
                }
              ]
            }
          }
        },
        {
          "displayName": "grantor",
          "keyword": "grantor",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "All Grantors"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "6dd85412",
      "displayName": "Specified User Options -- Permissions",
      "queryLines": [
        "SELECT DISTINCT grantee, grantor\n",
        "FROM system.information_schema.table_privileges"
      ]
    },
    {
      "name": "2b44458c",
      "displayName": "Privilege Types -- Permissions",
      "queryLines": [
        "SELECT \"All Types\" as privilege_options\n",
        "UNION ALL\n",
        "SELECT \"All Privileges\"\n",
        "UNION ALL\n",
        "SELECT \"Read\"\n",
        "UNION ALL\n",
        "SELECT \"Write\"\n"
      ]
    },
    {
      "name": "f47851f0",
      "displayName": "Tables from both Masks and Filters -- Fine-grained",
      "queryLines": [
        "WITH combined AS (\n",
        "    SELECT\n",
        "        CONCAT(cm.table_catalog, '.', cm.table_schema, '.', cm.table_name) AS full_name,\n",
        "        any_value(tables.table_owner) AS table_owner,\n",
        "        array_join(collect_list(cm.column_name), ', ') AS column_names,\n",
        "        'Column Masks' AS source\n",
        "    FROM \n",
        "        system.information_schema.column_masks cm\n",
        "    INNER JOIN\n",
        "        system.information_schema.tables tables\n",
        "    ON\n",
        "        CONCAT(cm.table_catalog, '.', cm.table_schema, '.', cm.table_name) = CONCAT(tables.table_catalog, '.', tables.table_schema, '.', tables.table_name)\n",
        "    GROUP BY \n",
        "        CONCAT(cm.table_catalog, '.', cm.table_schema, '.', cm.table_name)\n",
        "\n",
        "    UNION ALL\n",
        "\n",
        "    SELECT\n",
        "        CONCAT(rf.table_catalog, '.', rf.table_schema, '.', rf.table_name) AS full_name,\n",
        "        any_value(tables.table_owner) AS table_owner,\n",
        "        NULL AS column_names,\n",
        "        'Row Filters' AS source\n",
        "    FROM \n",
        "        system.information_schema.row_filters rf\n",
        "    INNER JOIN\n",
        "        system.information_schema.tables tables\n",
        "    ON\n",
        "        CONCAT(rf.table_catalog, '.', rf.table_schema, '.', rf.table_name) = CONCAT(tables.table_catalog, '.', tables.table_schema, '.', tables.table_name)\n",
        "    GROUP BY \n",
        "        CONCAT(rf.table_catalog, '.', rf.table_schema, '.', rf.table_name)\n",
        ")\n",
        "\n",
        "SELECT \n",
        "    full_name,\n",
        "    table_owner,\n",
        "    column_names,\n",
        "    source\n",
        "FROM \n",
        "    combined\n",
        "WHERE \n",
        "    source = :source_type OR :source_type = \"Both column masks and row filters\"\n",
        ";\n"
      ],
      "parameters": [
        {
          "displayName": "source_type",
          "keyword": "source_type",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "Both column masks and row filters"
                }
              ]
            }
          }
        }
      ]
    },
    {
      "name": "0fe3281a",
      "displayName": "Masks / Filters Toggle -- Fine-grained",
      "queryLines": [
        "SELECT \"Column Masks\"\n",
        "UNION ALL\n",
        "SELECT \"Row Filters\"\n",
        "UNION ALL\n",
        "SELECT \"Both column masks and row filters\""
      ]
    },
    {
      "name": "6bebf80e",
      "displayName": "Most Usages for both Masks and Filters -- Fine-grained",
      "queryLines": [
        "WITH\n",
        "  reads AS (\n",
        "    SELECT\n",
        "      CONCAT(lat.source_table_catalog, '.', lat.source_table_schema, '.', lat.source_table_name) AS full_name,\n",
        "      lat.created_by,\n",
        "      COUNT(*) AS usage_count\n",
        "    FROM\n",
        "      system.access.table_lineage lat\n",
        "    WHERE\n",
        "      lat.source_type = 'TABLE' AND\n",
        "      lat.source_table_name IS NOT NULL AND\n",
        "      lat.event_time >= :date_range_start AND lat.event_time <= :date_range_end\n",
        "    GROUP BY\n",
        "      lat.source_table_catalog, lat.source_table_schema, lat.source_table_name, lat.created_by\n",
        "  ),\n",
        "  filter_tables AS (\n",
        "    SELECT\n",
        "      CONCAT(rf.table_catalog, '.', rf.table_schema, '.', rf.table_name) AS full_name\n",
        "    FROM\n",
        "      system.information_schema.row_filters rf\n",
        "    WHERE\n",
        "        :filter_type = 'Row Filters' OR :filter_type = \"Both column masks and row filters\"\n",
        "    UNION ALL\n",
        "    SELECT\n",
        "      CONCAT(cm.table_catalog, '.', cm.table_schema, '.', cm.table_name) AS full_name\n",
        "    FROM\n",
        "      system.information_schema.column_masks cm\n",
        "    WHERE\n",
        "      :filter_type = 'Column Masks' OR :filter_type = \"Both column masks and row filters\"\n",
        "  ),\n",
        "  total_creators AS (\n",
        "    SELECT\n",
        "      COUNT(DISTINCT reads.created_by) AS total_creators\n",
        "    FROM\n",
        "      filter_tables ft\n",
        "    JOIN\n",
        "      reads\n",
        "      ON ft.full_name = reads.full_name\n",
        "    WHERE\n",
        "      reads.created_by IS NOT NULL\n",
        "  ),\n",
        "  top_creators AS (\n",
        "    SELECT\n",
        "      reads.created_by,\n",
        "      SUM(reads.usage_count) AS usage_count\n",
        "    FROM\n",
        "      filter_tables ft\n",
        "    JOIN\n",
        "      reads\n",
        "      ON ft.full_name = reads.full_name\n",
        "    WHERE\n",
        "      reads.created_by IS NOT NULL\n",
        "    GROUP BY\n",
        "      reads.created_by\n",
        "    ORDER BY\n",
        "      usage_count DESC\n",
        "    LIMIT 20\n",
        "  )\n",
        "SELECT\n",
        "  tc.created_by,\n",
        "  tc.usage_count,\n",
        "  tc_total.total_creators\n",
        "FROM\n",
        "  top_creators tc\n",
        "CROSS JOIN\n",
        "  total_creators tc_total;"
      ],
      "parameters": [
        {
          "displayName": "date_range_start",
          "keyword": "date_range_start",
          "dataType": "DATETIME",
          "defaultSelection": {
            "values": {
              "dataType": "DATETIME",
              "values": [
                {
                  "value": "now-12M/M"
                }
              ]
            }
          }
        },
        {
          "displayName": "date_range_end",
          "keyword": "date_range_end",
          "dataType": "DATETIME",
          "defaultSelection": {
            "values": {
              "dataType": "DATETIME",
              "values": [
                {
                  "value": "now/s"
                }
              ]
            }
          }
        },
        {
          "displayName": "filter_type",
          "keyword": "filter_type",
          "dataType": "STRING",
          "defaultSelection": {
            "values": {
              "dataType": "STRING",
              "values": [
                {
                  "value": "Both column masks and row filters"
                }
              ]
            }
          }
        }
      ]
    }
